{
  "kind": "collectionType",
  "collectionName": "combinator_queries",
  "info": {
    "name": "CombinatorQuery"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "property": {
      "type": "string",
      "required": true
    },
    "operator": {
      "type": "enumeration",
      "enum": [
        "equals",
        "not_equals",
        "contains",
        "not_contains",
        "greater_than",
        "less_than"
      ]
    },
    "value": {
      "type": "string"
    },
    "combinator": {
      "via": "queries",
      "model": "combinator"
    },
    "field": {
      "model": "dataset-field"
    },
    "state": {
      "type": "enumeration",
      "enum": ["pending", "done"],
      "default": "pending"
    },
    "state_msg": {
      "type": "string"
    },
    "state_at": {
      "type": "datetime"
    }
  }
}
