{
  "kind": "collectionType",
  "collectionName": "datasets",
  "info": {
    "name": "Dataset"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "citation": {
      "type": "text"
    },
    "link": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "source": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false
    },
    "title_layout": {
      "type": "text"
    },
    "summary_layout": {
      "type": "text"
    },
    "details_layout": {
      "type": "text"
    },
    "link_layout": {
      "type": "text"
    },
    "category": {
      "model": "category",
      "via": "datasets"
    },
    "combinators": {
      "via": "dataset",
      "collection": "combinator"
    },
    "fields": {
      "via": "dataset",
      "collection": "dataset-field"
    },
    "features": {
      "collection": "dataset-feature",
      "via": "dataset"
    },
    "process": {
      "type": "enumeration",
      "enum": [
        "pending",
        "active",
        "failed",
        "complete"
      ],
      "default": "pending"
    },
    "process_notes": {
      "type": "string"
    },
    "process_at": {
      "type": "datetime"
    },
    "refresh": {
      "type": "enumeration",
      "enum": [
        "pending",
        "active",
        "failed",
        "complete"
      ],
      "default": "pending"
    },
    "refresh_notes": {
      "type": "string"
    },
    "refresh_at": {
      "type": "datetime"
    },
    "old_id": {
      "type": "integer",
      "private": true
    },
    "old_name": {
      "type": "string",
      "private": true
    },
    "old_user_id": {
      "type": "integer",
      "private": true
    },
    "old_category_id": {
      "type": "integer",
      "private": true
    }
  }
}
