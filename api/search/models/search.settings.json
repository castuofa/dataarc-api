{
  "kind": "collectionType",
  "collectionName": "searches",
  "info": {
    "name": "Search"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "citation": {
      "type": "text"
    },
    "url": {
      "type": "string"
    },
    "filters": {
      "type": "json"
    },
    "results": {
      "type": "json"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "searches"
    }
  }
}
