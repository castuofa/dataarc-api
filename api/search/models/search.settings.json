{
  "kind": "collectionType",
  "collectionName": "searches",
  "info": {
    "name": "Search",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "filters": {
      "type": "json"
    },
    "path": {
      "type": "string"
    },
    "expires": {
      "type": "datetime"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "processed": {
      "type": "datetime"
    },
    "process": {
      "type": "boolean",
      "default": false,
      "private": true
    },
    "busy": {
      "type": "boolean",
      "default": false,
      "private": true
    }
  }
}
