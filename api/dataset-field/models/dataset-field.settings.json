{
  "kind": "collectionType",
  "collectionName": "dataset_fields",
  "info": {
    "name": "DatasetField"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "path": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "string",
        "number",
        "boolean",
        "array",
        "url",
        "start_date",
        "end_date",
        "text_date"
      ],
      "required": true,
      "default": "string"
    },
    "state": {
      "type": "enumeration",
      "enum": ["pending", "missing", "complete"],
      "default": "pending"
    },
    "dataset": {
      "model": "dataset",
      "via": "fields"
    },
    "old_id": {
      "type": "integer",
      "private": true
    },
    "old_dataset_id": {
      "type": "integer",
      "private": true
    }
  }
}
