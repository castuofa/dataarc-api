{
  "kind": "collectionType",
  "collectionName": "combinators",
  "info": {
    "name": "Combinator"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "citation": {
      "type": "text"
    },
    "operator": {
      "type": "enumeration",
      "enum": [
        "and",
        "or"
      ],
      "default": "and"
    },
    "queries": {
      "collection": "combinator-query",
      "via": "combinator"
    },
    "concepts": {
      "via": "combinators",
      "collection": "concept"
    },
    "features": {
      "via": "combinators",
      "collection": "feature"
    },
    "dataset": {
      "model": "dataset",
      "via": "combinators"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "pending",
        "running",
        "failed",
        "done"
      ],
      "default": "pending"
    },
    "state_msg": {
      "type": "string"
    },
    "state_at": {
      "type": "datetime"
    },
    "old_id": {
      "type": "integer",
      "private": true
    },
    "old_dataset_id": {
      "type": "integer",
      "private": true
    },
    "old_user_id": {
      "type": "integer",
      "private": true
    }
  }
}
