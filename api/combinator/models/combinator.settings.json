{
  "kind": "collectionType",
  "collectionName": "combinators",
  "info": {
    "name": "Combinator"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "citation": {
      "type": "text"
    },
    "url": {
      "type": "string"
    },
    "operator": {
      "type": "enumeration",
      "enum": [
        "and",
        "or"
      ]
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "pending",
        "running",
        "failed",
        "done"
      ],
      "default": "pending"
    },
    "state_msg": {
      "type": "string"
    },
    "state_at": {
      "type": "datetime"
    },
    "queries": {
      "via": "combinator",
      "collection": "combinator-query"
    },
    "concepts": {
      "collection": "concept",
      "via": "combinators",
      "dominant": true
    },
    "features": {
      "collection": "feature",
      "via": "combinators",
      "dominant": true
    },
    "dataset": {
      "model": "dataset",
      "via": "combinators"
    }
  }
}
