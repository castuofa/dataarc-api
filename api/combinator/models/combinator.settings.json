{
  "kind": "collectionType",
  "collectionName": "combinators",
  "info": {
    "name": "Combinator"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "citation": {
      "type": "text"
    },
    "operator": {
      "type": "enumeration",
      "enum": [
        "AND",
        "OR"
      ],
      "default": "AND"
    },
    "queries": {
      "via": "combinator",
      "collection": "combinator-query"
    },
    "concepts": {
      "via": "combinators",
      "collection": "concept"
    },
    "dataset": {
      "via": "combinators",
      "model": "dataset"
    },
    "features": {
      "via": "combinators",
      "collection": "dataset-feature",
      "dominant": true
    },
    "user": {
      "via": "combinators",
      "plugin": "users-permissions",
      "model": "user"
    },
    "old_id": {
      "type": "integer",
      "private": true
    },
    "old_dataset_id": {
      "type": "biginteger",
      "private": true
    },
    "old_user_id": {
      "type": "integer",
      "private": true
    }
  }
}
