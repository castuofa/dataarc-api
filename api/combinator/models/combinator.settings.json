{
  "kind": "collectionType",
  "collectionName": "combinators",
  "info": {
    "name": "Combinator"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "citation": {
      "type": "text"
    },
    "operator": {
      "type": "enumeration",
      "enum": ["AND", "OR"],
      "default": "AND"
    },
    "queries": {
      "collection": "combinator-query",
      "via": "combinator"
    },
    "concepts": {
      "collection": "concept",
      "via": "combinators"
    },
    "dataset": {
      "model": "dataset",
      "via": "combinators"
    },
    "features": {
      "collection": "dataset-feature",
      "via": "combinators",
      "dominant": true
    },
    "refresh": {
      "type": "enumeration",
      "enum": ["pending", "active", "failed", "complete"],
      "default": "pending"
    },
    "refresh_notes": {
      "type": "string"
    },
    "refresh_at": {
      "type": "datetime"
    },
    "old_id": {
      "type": "integer",
      "private": true
    },
    "old_dataset_id": {
      "type": "biginteger",
      "private": true
    },
    "old_user_id": {
      "type": "integer",
      "private": true
    }
  }
}
