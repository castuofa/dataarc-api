{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": ["info", "warning", "error"],
      "index": true
    },
    "controller": {
      "type": "string",
      "index": true
    },
    "action": {
      "type": "string",
      "index": true
    },
    "item": {
      "type": "string",
      "index": true
    },
    "details": {
      "type": "text"
    },
    "payload": {
      "type": "json"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "autoPopulate": false
    }
  }
}
