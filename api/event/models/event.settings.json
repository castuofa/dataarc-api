{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "type": {
      "type": "string"
    },
    "action": {
      "type": "enumeration",
      "enum": ["create", "update", "delete", "process", "refresh"]
    },
    "item": {
      "type": "string"
    },
    "details": {
      "type": "text"
    },
    "payload": {
      "type": "json"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
