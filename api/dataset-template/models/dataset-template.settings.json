{
  "kind": "collectionType",
  "collectionName": "dataset_templates",
  "info": {
    "name": "DatasetTemplate"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "layout": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": ["title", "map", "result", "link"]
    },
    "dataset": {
      "via": "templates",
      "model": "dataset"
    }
  }
}
