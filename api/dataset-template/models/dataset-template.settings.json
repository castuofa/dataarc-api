{
  "kind": "collectionType",
  "collectionName": "dataset_templates",
  "info": {
    "name": "DatasetTemplate"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "layout": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "title",
        "map",
        "result",
        "link"
      ]
    },
    "dataset": {
      "via": "templates",
      "model": "dataset"
    },
    "user": {
      "via": "dataset_templates",
      "plugin": "users-permissions",
      "model": "user"
    },
    "old_id": {
      "type": "integer",
      "private": true
    },
    "old_dataset_id": {
      "type": "integer",
      "private": true
    },
    "old_user_id": {
      "type": "integer",
      "private": true
    }
  }
}
